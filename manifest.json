{
  "name": "Eksi Normallestirici",
  "manifest_version": 2,
  "version": "0.99",
  "description": "Ekşi Sözlük renklerini orijinal haline döndürür",
  "author": "Serdar Cevher",
  "permissions": ["storage"],
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "browser_action": {
    "name": "Eksi Normallestirici",
    "icons": ["icon.png"],
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://eksisozluk.com/*"],
      "js": ["jquery.min.js","script.js"],
      "css": ["fix.css"]
    }
  ]
}
